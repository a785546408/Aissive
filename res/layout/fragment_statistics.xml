<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.my1.MainActivity" >

    <LinearLayout
        android:id="@+id/MainLayout2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/machineid"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:text="设备编号：NULL"
                android:layout_weight="1.2"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/stationid"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="车站编号：NULL "
                android:layout_weight="1.2"
                android:textSize="15sp" />

            <Chronometer
                android:id="@+id/chronometer2"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:textColor="#1e0903"
                android:layout_weight="1"
                android:textSize="15sp"
                android:textStyle="bold" 
                />




        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.2" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.2"
                android:background="@drawable/line_06" />
            
            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.2"
                android:background="@drawable/line_06" />
            
            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/line_06" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/pgoin"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_weight="1.1"
                android:gravity="center_vertical"
                android:text="进        0"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/passengerflow"
                android:layout_width="75dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="客流"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/pgoout"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_weight="1.1"
                android:gravity="center_vertical"
                android:text="0        出"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/tgoin"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_weight="1.1"
                android:gravity="center_vertical"
                android:text="进        0"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/transaction"
                android:layout_width="75dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="交易"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/tgout"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_weight="1.1"
                android:gravity="center_vertical"
                android:text="0        出"
                android:textSize="13sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.1" >

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/line_11" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/line_11" />

        </LinearLayout>
    </LinearLayout>
    
    
    
    
    
    
    
    
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scrollbars="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/dianid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/line_26" />

                <TextView
                    android:id="@+id/button1"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="40dp"
                    android:text="通行统计"
                    android:textSize="12sp" />

                <ImageView
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="90dp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/san_03" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:id="@+id/imglayout"
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4" />

                    <ImageView
                        android:id="@+id/maximg"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/biao1_03" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <ImageView
                        android:id="@+id/avgimg"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/biao1_06" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <ImageView
                        android:id="@+id/minimg"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/biao1_11" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.2" />

                    <ImageView
                        android:id="@+id/conimg"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/biao1_14" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.2" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/inlayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/imglayout"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/instream"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="进闸客流：" />

                    <TextView
                        android:id="@+id/maxinstream"
                        android:layout_width="8dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="人/min"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/avginstream"
                        android:layout_width="8dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="人/min"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/mininstream"
                        android:layout_width="8dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="人/min"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/coninstream"
                        android:layout_width="8dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="人/min"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="12sp" />

                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_toRightOf="@id/inlayout"
                        android:background="@drawable/san_07"
                        android:gravity="center" />
                </LinearLayout>

                <TextView
                    android:id="@+id/t1"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_below="@id/inlayout"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:background="#00EE76" />

                <LinearLayout
                    android:id="@+id/outlayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/t1"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/outstream"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="出闸客流：" />

                    <TextView
                        android:id="@+id/maxoutstream"
                        android:layout_width="8dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="人/min"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/avgoutstream"
                        android:layout_width="8dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="人/min"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/minoutstream"
                        android:layout_width="8dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="人/min"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/conoutstream"
                        android:layout_width="8dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:text="人/min"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="12sp" />

                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_toRightOf="@id/outlayout"
                        android:background="@drawable/san_07"
                        android:gravity="center" />
                </LinearLayout>

                <TextView
                    android:id="@+id/t2"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_below="@id/outlayout"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:background="#00EE76" />

                <LinearLayout
                    android:id="@+id/Viewlayout"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/t2" >

                    <CustomView.MyView
                        android:id="@+id/line"
                        android:layout_width="wrap_content"
                        android:layout_height="260dp"
                        android:layout_centerInParent="true" >
                    </CustomView.MyView>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/Viewlayout2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/Viewlayout" >

                    <CustomView.HorizontalBarView
                        android:id="@+id/horizontalBar1"
                        android:layout_width="wrap_content"
                        android:layout_height="300dp"
                        android:adjustViewBounds="true" />
                </LinearLayout>
            </RelativeLayout>

            
        </LinearLayout>
    </ScrollView>

</LinearLayout>